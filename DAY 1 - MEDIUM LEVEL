#include <iostream>
#include <string>
using namespace std;


bool isVowel(char ch) {
    ch = tolower(ch);
    return (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u');
}

int main() {
    int T;
    cin >> T; 
    
    while (T--) {
        int N;
        string S;
        
        cin >> N; 
        cin >> S; 
        
        int consonantCount = 0;
        bool isHard = false;
        
        for (int i = 0; i < N; i++) {
            if (!isVowel(S[i])) {
                consonantCount++;
                if (consonantCount >= 4) {
                    isHard = true;
                    break;
                }
            } else {
                consonantCount = 0; 
            }
        }
        
        if (isHard) {
            cout << "NO" << endl;
        } else {
            cout << "YES" << endl;
        }
    }
    
    return 0;
}
