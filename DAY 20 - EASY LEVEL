#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
int T;
    cin >> T;  

    while (T--) {
        int N;
        cin >> N;  
        
        int start38_count = 0;
        int ltime108_count = 0;

        for (int i = 0; i < N; i++) {
            string contestCode;
            cin >> contestCode;

            
                start38_count++;
            } else if (contestCode == "LTIME108") {
                ltime108_count++;
            }
        }

        
        cout << start38_count << " " << ltime108_count << endl;
    }

    return 0;
}
