#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
int T; 
    cin >> T;
    
    while (T--) {
        int N, K;
        cin >> N >> K;
        
        int count = 0; 
        for (int i = 0; i < N; i++) {
            int characteristic;
            cin >> characteristic;
            
            int newCharacteristic = characteristic + K;
            
            if (newCharacteristic % 7 == 0) {
                count++;
            }
        }
        
        cout << count << endl;
    }
    
    return 0;
}
