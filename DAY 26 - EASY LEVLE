#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
int T;
   
    cin >> T;
    
    while (T--) {
        int P, Q;
        
        cin >> P >> Q;
        
        int totalPoints = P + Q;
        
        int servePair = totalPoints / 2;
        
        if (servePair % 2 == 0) {
            cout << "Alice" << endl;
        } else {
            cout << "Bob" << endl;
        }
    }
    
    return 0;
}
